{
    "name": "xresources",
    "displayName": "Xresources",
    "description": "syntax highlighting for Xresources",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {"url": "https://github.com/antcap96/xresources"},
    "publisher": "antcap96",
    "contributes": {
        "languages": [{
            "id": "xdefaults",
            "aliases": ["Xdefaults", "xdefaults"],
            "extensions": [".Xresources"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xdefaults",
            "scopeName": "source.xdefaults",
            "path": "./syntaxes/xdefaults.tmLanguage.json"
        }]
    }
}