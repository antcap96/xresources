{
    "name": "xresources",
    "displayName": "Xresources",
    "description": "syntax highlighting for Xresources",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xdefaults",
            "aliases": ["Xdefaults", "xdefaults"],
